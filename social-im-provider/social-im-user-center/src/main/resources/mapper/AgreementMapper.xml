<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.enuos.live.mapper.AgreementMapper">

    <!--获取url-->
    <select id="getUrl" parameterType="java.util.Arrays" resultType="java.util.Map">
        select type, content from tb_agreement where type in
        <foreach collection="array" item="type" open="(" close=")" separator=",">
            #{type}
        </foreach>
    </select>
</mapper>